<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="dao.EducationEntity" table="education" schema="lawfirm">
        <id name="educationId">
            <column name="education_id" sql-type="int(4) unsigned"/>
        </id>
        <property name="grade">
            <column name="grade" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <set name="employeesByEducationId" inverse="true">
            <key>
                <column name="education_id"/>
            </key>
            <one-to-many not-found="ignore" class="dao.EmployeeEntity"/>
        </set>
    </class>
</hibernate-mapping>